    <nav class="navbar " style="margin-bottom: 0px;">
        <div class="container " style="padding-left: 0px; padding-right: 0px;">
            <div class="navbar-header" style="width: 100%">
                <div class="col-md-6" style="padding-right: 2.5px; padding-top: 0px; padding-bottom: 0px; padding-left: 0px;">
                    <div class="card-panel-header-menu">
                        <div class="header-page-title-text text-light-primary pull-left clearfix" style="padding-right: 10px;">
                                  <img src="~/images/logo.png" alt="Logo" style="height: 40px;">
                            DTASK
                        </div>
                        <div class="text-light-primary pull-left clearfix" style="padding-top: 18px; font-weight: 400; font-size: 14px;">
                        </div>
                        <div class="pull-right clearfix" style="padding-top: 10px;">
                            @*the menu goes here*@
                        </div>
                    </div>
                </div>

                <div class="col-md-6 " style="padding-right: 0px; padding-top: 0px; padding-bottom: 0px; padding-left: 2.5px;">

                    <div class="card-panel-header-titlefunction">
                        <div class="header-page-title-text  text-light-primary pull-left">
                            Mapa Interactivo
                        </div>
                        <div class="header-functions-text pull-right">
                            
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </nav>
    <!-- START MAIN BS CONTAINER -->
    <div class="container body-content">

        <div id="fsLoader" class="backdrop" style="display: none">
            <div class="col-xs-offset-0 col-xs-12 col-md-offset-4 col-md-4 text-center verticalcenter ">
                <div class="card-panel ">
                    <div class="preloader-wrapper big active">
                        <div class="spinner-layer spinner-blue-only">
                            <div class="circle-clipper left">
                                <div class="circle"></div>
                            </div>
                            <div class="gap-patch">
                                <div class="circle"></div>
                            </div>
                            <div class="circle-clipper right pull-right">
                                <div class="circle"></div>
                            </div>
                        </div>
                    </div>
                    <br />
                    <span id="fsLoaderMessage">Procesando...</span>
                </div>
            </div>
        </div>


        <div style="margin-top: 0px">
            @RenderBody() 
        </div>

        <footer class="container">
            <div class="row">
                <div class="col-md-12 no-padding">
                    <div class="col-xs-6 col-sm-6 col-md-6 no-padding">
                        <img src="~/images/logo.png" style="height: 70px;" class="pull-left" id="idLogoDTOP" />
                    </div>

                    <div class="col-xs-6 col-sm-6 col-md-6 no-padding">
                    </div>
                </div>
            </div>
        </footer>

    </div>
    <!-- END MAIN BS CONTAINER -->

    <div class="modal fade" id="ItemSearchModal" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-body no-padding">
                </div>
            </div>
        </div>
    </div>
