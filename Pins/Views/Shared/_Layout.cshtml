<body style="background-color: rgb(216, 216, 216)">
    <style type="text/css">
        /* REPORT STATUS LOGS */
        .icono {
            font-size: 32px;
        }

            .icono.nuevo {
                color: #a5a5a5;
            }

            .icono.sometido {
                color: #ffc000;
            }

            .icono.regional {
                color: #b97a56;
            }

            .icono.central {
                color: #31859b;
            }

            .icono.auditoria {
                color: #ea700d;
            }

            .icono.pre-intervencion, .icono.completado {
                color: #0070c0;
            }

            .icono.pagaduria {
                color: #9dbb61;
            }

            .icono.denegado {
                color: #C05046 !important;
            }

        /* END REPORT STATUS LOGS */


        .dropdown-submenu {
            position: relative;
        }

            .dropdown-submenu .dropdown-menu {
                top: 0;
                left: 100%;
                margin-top: -1px;
                padding-left: 5px;
                /* width: 200px;*/
            }

        .userInfoSection {
            margin-left: 10px;
            margin-right: 10px;
            text-align: center;
            margin-top: 8px;
            border-bottom: solid 1px rgba(128, 128, 128, 0.160784313725);
        }

        .bold {
            font-weight: bold !important;
        }

        .orange-highlight {
            background-color: orange;
        }

        .hidden-info {
            display: none;
        }
    </style>
